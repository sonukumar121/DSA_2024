#include<stdio.h>
#include<stdlib.h>
struct node
{
	int data;
	struct node *next;
}*start=NULL;

void create()
{
struct node *tmp,*tmp1;
 tmp=(struct node*)malloc(sizeof(struct node));
 
 printf("enter the data= ");
 scanf("%d",&tmp->data);
 tmp->next=NULL;
 if(start==NULL)
 {
   start=tmp;
   tmp1=tmp;	
 }
 	
 else
 {
  tmp1=start;
  
  while(tmp1->next!=NULL)
  {
   tmp1=tmp1->next;
  }
  
   tmp1->next=tmp;
  }

struct binsert()
{//linked
  int tmp*,*head;
  while(tmp->next!=NULL)
  
  {
  	tmp=tmp->next;
	  }	
	  tmp1=tmp;
	  tmp->next=start;
	  tmp=malloc(sizeof(struct node));
	  tmp1=start;
	  
	  //insert
	  tmp=malloc(sizeof(Struct node));
	  tmp1=start;
	  while(tmp1->next!=start)
	  {
	  	tmp1=tmp1->next;
	  }
	  tmp->next=start;
	  head=start;
	  tmp1->next=tmp;
}

