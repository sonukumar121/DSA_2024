

// Insertion  SORTING.

#include<stdio.h>

int main()
{
  int a[20], i, j, k, x, n,c;
  printf ("enter the elements= ");
  for (i = 0; i < 5; i++)
	{
	  scanf ("%d", &a[i]);
	}
  for (i = 0; i < 5; i++)
	{
	  for (j = i + 1; j < 5; j++)
		{
		  if (a[i] > a[j])
			{
			  c = a[i];
			  a[i] = a[j];
			  a[j] = c;
			}

		}
	}
  for (i = 0; i < 5; i++)
       printf("%d",a[i]);	

	}






//Selection_Sorting..
#include<stdio.h>
int a[5];
int min=a[1];
int main()
{
	int a[6],i,j,x,y,min,c;
	printf("enter the elemnets = ");
	for(i=1;i<7;i++)
	{
		scanf("%d",&a[i]);
	}
	for(x=1;x<6;x++)
	
	{
		for(i=x+1;i<6;i++)
		{
		 
		  if(min>a[i+1])
		  
		  {
		  	min=a[i+1];
		  	y=i+1;
		  	
		  }	
		}
		c=a[y];
		a[y]=a[x];
		a[x]=c;
		x=x+1;
	}
	for(i=1;i<7;i++)
	{
		printf("%d, ",a[i]);
	}

} //



//#include<stdio.h>
//bubble sorting

int main()
{
  int a[5], i, j, k, x, n,c;
  printf ("enter the elements= ");
  for (i = 0; i < 5; i++)
	{
	  scanf ("%d", &a[i]);
	}
	printf("after calculations ");
  for (i = 0; i < 5; i++)
	{
	  for (j =0 ; j < 5; j++)
		{
		  if (a[j] > a[j+1])
			{
			  c = a[j+1];
			  a[j+1] = a[j];
			  a[j] = c;
			}

		}
	
		
	}
  for (j = 0; j < 5; j++)
  {
  	printf("%d",a[j]);
  }
       	

	}
