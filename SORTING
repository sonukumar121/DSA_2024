//SORTING==>SIMPLE METHOD.

#include<stdio.h>
int main()
{
    int a[20],i,j,x,n,c;
    printf("enter the elements= ");
    for(i=0;i<5;i++)
    {
         scanf("%d",&a[i]);//6,7,11,2,1. 
    }
  


for(i=0;i<5;i++)
{
    for(j=i+1;j<5;j++)
    {
        if(a[i]>a[j])
        {
            c=a[i];
            a[i]=a[j];
            a[j]=c;
        }
    }
}

for(i=0;i<5;i++)
{
  printf("%d,",a[i]);  
}
}




// BUBBLE SORTING.

#include<stdio.h>

int main()
{
  int a[20], i, j, k, x, n,c;
  printf ("enter the elements= ");
  for (i = 0; i < 5; i++)
	{
	  scanf ("%d", &a[i]);
	}
  for (i = 0; i < 5; i++)
	{
	  for (j = i + 1; j < 5; j++)
		{
		  if (a[i] > a[j])
			{
			  c = a[i];
			  a[i] = a[j];
			  a[j] = c;
			}

		}
	}
  for (i = 0; i < 5; i++)
       printf("%d",a[i]);	

	}














#include<stdio.h>
int main()
{
    int a[20],i,j,x,n,c,min;
    printf("enter the elements=" );
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    min=a[0];
    for(x=0;x<n;x++)
    {
        for(i=0;i<n;i++)
        {
          if(a[i]<a[i+1])
          {
              min=a[i];
              c=a[i];
              a[i]=min;
              min=c;
          }
          
        }
        for(i=0;i<=5;i++)
        {
            printf("%d",a[i]);
        }
        
        
        
        
    }
}
















//SELECTION SORTINNG

#include<stdio.h>
int main()

{
    int a[20],i,j,x,n,min,t,c;
    printf("enter the elements= ");
    for(i=0;i<=6;i++)
    {
        scanf("%d",&a[i]);
    }

    min=a[1];
    x=0;

    printf("lets find  min elements in array is \n");


   
   for(x=0;x<=6;x++)
   {
    for(i=x+1;i<=6;i++)
    {
        if(min<a[i])
        {
            continue;
        }
        else
        min=a[i];
        t=min; //t=1
    }
    
    //printf("%d\n",t);
   c=a[i];
   a[i]=a[x];
   a[x]=c;
    x=x+1;

  }
  for(i=0;i<=6;i++)
  {
      printf("%d, ",a[i]);
  }



}
