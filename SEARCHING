

// LINEAR SERCHING==>SIMPLE METHODS.
#include<stdio.h>
// linear searching.
int main()
{
    int a[20],i,j,x,n;
    printf("enter the elements= ");
    for(i=0;i<5;i++)
    {
       scanf("%d",&a[i]);  
    }
    printf("enter the searching elemnts= ");
    scanf("%d",&x);
   
   for(i=0;i<5;i++)
   {
       if(a[i]==x)
       {
           printf("elements is found");
           break;
       }
       if(i==4)
       {
           printf("elemets is not found");
       }
       
   }
 
   
}

